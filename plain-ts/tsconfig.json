{
    "compilerOptions":{
        "esModuleInterop": true,
        "outDir": "./dist/",
        "noImplicitAny": true,
        "strictNullChecks": true,
        "module": "es6",
        "target": "es5",
        "jsx": "react",
        "allowJs": true,
        "sourceMap": true,
        "typeRoots": ["node_modules/@types"],
        "baseUrl": "typescript",
        "paths": {
            "@Conway/*": ["*"]
        }
    },
    "include": [
        "**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}
